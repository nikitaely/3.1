{ "parser": "@babel/eslint-parser",
  
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": ["warn"],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0
  }
  
}
